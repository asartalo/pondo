{
  "scripts": {
    "rails_cypress_server": "RAILS_ENV=test bin/rails s -p 5002",
    "cypressspec": "cd ./spec && ../node_modules/.bin/cypress open",
    "cypressrun": "cd ./spec && ../node_modules/.bin/cypress run --parallel --record",
    "cypressci": "npm-run-all --parallel rails_cypress_server cypressrun"
  },
  "dependencies": {
    "@rails/webpacker": "3.5",
    "coffee-loader": "^0.9.0",
    "coffeescript": "1.12.7",
    "cypress-capybara": "^0.0.2",
    "jquery": "^3.3.1",
    "npm-run-all": "^4.1.3",
    "rails-erb-loader": "^5.5.0"
  },
  "devDependencies": {
    "cypress": "^3.1.0",
    "cypress-cucumber-preprocessor": "^1.5.0",
    "webpack-dev-server": "2.11.2"
  }
}
